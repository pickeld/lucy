# App Configurations
LOG_LEVEL=DEBUG
WEBHOOK_URL=http://app:8765/webhook
DALLE_PREFIX=!!
CHAT_PREFIX=??

# PostgreSQL Configurations (for LangGraph)
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_DB=letta
POSTGRES_USER=letta
POSTGRES_PASSWORD=letta_pass

#Letta Configurations (legacy - can be removed after migration)
LETTA_DEBUG=False
LETTA_HOST=letta
LETTA_PORT=8283
MEMORY__PASSAGE_ARCHIVAL__ENABLED=true
MEMORY__PASSAGE_ARCHIVAL__MIN_MESSAGES=30
MEMORY__PASSAGE_ARCHIVAL__MIN_DAYS_OLD=1
MEMORY__PASSAGE_ARCHIVAL__SUMMARIZE_TOOL=openai:gpt-4

# Redis Configurations
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_TTL=604800

# WAHA Configurations
WAHA_SESSION_NAME=default
WAHA_BASE_URL=http://waha:3000
WAHA_API_KEY=your-waha-api-key
WAHA_PRINT_QR=false
WAHA_MEDIA_STORAGE=LOCAL
WHATSAPP_FILES_FOLDER=/app/.media
WHATSAPP_FILES_LIFETIME=0
WHATSAPP_DOWNLOAD_MEDIA=true
WHATSAPP_FILES_MIMETYPES=image/jpeg,image/png

# Others
EMBEDDING_MODEL=paraphrase-multilingual-MiniLM-L12-v2
DALLE_MODEL=dall-e-3
TAVILY_API_KEY=your-tavily-api-key
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o
OPENAI_TEMPERATURE=0.7

# LangSmith (Optional - for web-based visualization)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your-langsmith-api-key
LANGCHAIN_PROJECT=whatsapp-gpt
